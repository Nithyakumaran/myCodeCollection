package com.edit.distance;

public class Atoi {
	public static void main(String[] args){
		System.out.println("Program to print Atoi");
		char[] chr= {'8','7','6','5'};
		int val= atoi(chr);
		System.out.println(val);
	}
	public static int atoi(char[] c){
		
		if(c.length==0) return 0;
		int res=0;
		for(int i=0;i<c.length;i++){
			res=res*10+ (c[i]-'0');
		}
		return res;
	}
}
